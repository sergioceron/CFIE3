    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>panel</from-outcome>
            <to-view-id>/panel.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/proyectos/new.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>list</from-outcome>
            <to-view-id>/proyectos/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/proyectos/list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>view</from-outcome>
